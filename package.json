{
  "name": "commonform-docx",
  "description": "export Common Form projects to Open Office XML",
  "version": "0.3.0",
  "author": {
    "name": "Kyle E. Mitchell",
    "email": "kyle@kemitchell.com",
    "url": "http://kemitchell.com/"
  },
  "bugs": {
    "url": "https://github.com/commonform/commonform-docx/issues"
  },
  "dependencies": {
    "commonform-decimal-numbers": "1.x",
    "commonform-flatten": "1.x",
    "commonform-number": "1.x",
    "commonform-resolve": "1.x",
    "concat-stream": "^1.4.7",
    "jszip": "^2.4.0",
    "smart-quotes": "0.0.1"
  },
  "devDependencies": {
    "check-versions": "1.x",
    "jscs": "^1.11.3",
    "jshint": "^2.6.0"
  },
  "homepage": "https://github.com/commonform/commonform-docx",
  "keywords": [
    "contracts",
    "document assembly",
    "knowledge management",
    "law"
  ],
  "license": "Apache-2.0",
  "main": "source/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/commonform/commonform-docx.git"
  },
  "scripts": {
    "build": "node build/scaffold.js data/scaffold.docx > data/scaffold.json",
    "check": "check-versions --check-module",
    "lint": "jshint source && jscs source",
    "pre-commit": "npm run lint && npm run check",
    "prepublish": "npm run build",
    "travis-ci": "npm run pre-commit"
  }
}
