{
  "name": "commonform-docx",
  "description": "render Common Forms in Office Open XML",
  "version": "0.4.2",
  "author": "Kyle E. Mitchell <kyle@kemitchell.com> (http://kemitchell.com)",
  "bugs": "https://github.com/commonform/commonform-docx/issues",
  "dependencies": {
    "commonform-flatten": "^0.4.0",
    "concat-stream": "^1.4.7",
    "decimal-numbering": "^1.0.0",
    "jszip": "^2.4.0",
    "merge": "^1.2.0",
    "smart-quotes": "0.0.2"
  },
  "devDependencies": {
    "check-versions": "^1.0.0",
    "jscs": "^1.11.3",
    "jshint": "^2.6.0"
  },
  "homepage": "https://commonform.github.io",
  "keywords": [
    "Microsoft Office",
    "Microsoft Word",
    "Office Open XML",
    "contracts",
    "document assembly",
    "docx",
    "knowledge management",
    "law"
  ],
  "license": "Apache-2.0",
  "main": "source/index",
  "peerDependencies": {
    "commonform": "1.x"
  },
  "repository": "commonform/commonform-docx",
  "scripts": {
    "build": "node build/scaffold.js data/scaffold.docx > data/scaffold.json",
    "check": "check-versions --check-module",
    "lint": "jshint --config .dotfiles/jshint.json source && jscs --config .dotfiles/jscs.json source",
    "precommit": "npm run check && npm run lint && npm run prepublish",
    "prepublish": "npm run build"
  }
}
